{
  "name": "mvw-navigation",
  "version": "3.0.0",
  "description": "Module to generate a site navigation from a given structure file.",
  "keywords": [
    "website",
    "navigation",
    "structure"
  ],
  "license": "MIT",
  "author": {
    "name": "tiliavir",
    "email": "tiliavir@gmail.com"
  },
  "types": "js/index.d.ts",
  "main": "js/index.js",
  "devDependencies": {
    "@types/node": "11.13.0",
    "@types/jasmine": "3.3.12",
    "@types/jsonschema": "1.1.1",
    "@types/pug": "2.0.4",
    "jasmine": "3.4.0",
    "typescript": "3.4.2",
    "tslint": "5.15.0",
    "typescript-json-schema": "0.37.0"
  },
  "dependencies": {
    "jsonschema": "~1.2.4",
    "pug": "~2.0.3"
  },
  "scripts": {
    "build": "tsc -p ./ts",
    "postbuild": "typescript-json-schema ./ts/tsconfig.json StructureConfig --titles --required --excludePrivate --noExtraProps > ./js/site-structure-schema.json",
    "test": "tslint --project ./ts && npm run build && tsc -p ./spec && jasmine"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tiliavir/mvw-navigation.git"
  },
  "bugs": {
    "url": "https://github.com/tiliavir/mvw-navigation/issues"
  },
  "homepage": "https://github.com/tiliavir/mvw-navigation#readme"
}
